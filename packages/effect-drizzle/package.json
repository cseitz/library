{
  "name": "@cseitz/effect-drizzle",
  "version": "0.0.1",
  "description": "Effect Drizzle ORM",
  "main": "./dist/index.cjs",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "benchmark": "tsc --project tsconfig.benchmark.json",
    "lint": "eslint --cache src",
    "typecheck": "tsc --noEmit --preserveWatchOutput --pretty",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "publishConfig": {
    "@library:registry": "http://localhost:4873"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.29.3",
  "devDependencies": {
    "tsdown": "^0.16.8",
    "typescript": "^5.9.3",
    "@effect/vitest": "^0.27.0",
    "@cseitz/effect-fn": "workspace:*",
    "@cseitz/effect-tag": "workspace:*"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "src",
    "README.md",
    "package.json",
    "!**/*.test.*",
    "!**/*.spec.*",
    "!**/__test__",
    "!**/__tests__"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "peerDependencies": {
    "typescript": ">=5.7.2",
    "effect": ">=3.19.17",
    "lodash-es": ">=4.17.22",
    "@types/lodash-es": ">=4.17.12",
    "@cseitz/effect-fn": ">=0.0.1",
    "@cseitz/effect-tag": ">=0.0.1",
    "@effect/sql": ">=0.49.0",
    "@effect/sql-pg": ">=0.50.1",
    "drizzle-orm": ">=1.0.0 || >=1.0.0-beta",
    "pg": ">=8.16.3"
  }
}
